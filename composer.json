{
  "name": "codeinc/lib-psr15middlewares",
  "version": "1.3.0",
  "description": "A collection a PSR-15 middlewares",
  "homepage": "https://github.com/CodeIncHQ/lib-psr15middlewares",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "micheh/psr7-cache": "^0.5.0",
    "guzzlehttp/psr7": "^1.4",
    "codeinc/lib-psr7responses": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {"CodeInc\\Psr15Middlewares\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Psr15Middlewares\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "suggest": {
    "relay/relay": "A PSR-15 request handler."
  }
}